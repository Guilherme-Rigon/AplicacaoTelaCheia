@{
    ViewData["Title"] = "Home Page";
}
<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <h2 class="display-3 text-danger text-center" id="temporizador"></h2>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>
<hr />
<div class="row">
    <div class="col">
        <button class="btn btn-outline-success btn-block" id="trocarPrivacy" onclick="TrocarPrivacy()">Trocar o conteudo</button>
    </div>
    <div class="col">
        <button class="btn btn-outline-primary btn-block" id="telaCheia" onclick="TelaCheia()">Tela Cheia</button>
    </div>
</div>
<script>
    window.onload = () => {
        $("#temporizador").text(countMaximo); //Exibe na tela o valor máximo do temporizador;
        Temporizador(); //Inicia o temporizador;
    }
    function TrocarPrivacy() {
        ResetarTemporizador(); //Reseta o temporizador;
        $.ajax({
            url: "/Home/Privacy",
            success: function (result) {
                $("#main").html(result);
            }
        })
    }
    function TelaCheia() {
        var elem = document.documentElement;
        if (elem.requestFullscreen) {
            elem.requestFullscreen();
        } else if (elem.webkitRequestFullscreen) {
            elem.webkitRequestFullscreen();
        } else if (elem.msRequestFullscreen) {
            elem.msRequestFullscreen();
        }
    }
</script>